{% extends 'base.html.twig' %}

{% block title %}Coordinate{% endblock %}

{% block body %}
    <h1>Coordinate</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ coordinate.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ coordinate.name }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ coordinate.adress }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ coordinate.zipCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ coordinate.city }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ coordinate.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ coordinate.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('coordinate_index') }}">back to list</a>

    <a href="{{ path('coordinate_edit', {'id': coordinate.id}) }}">edit</a>

    {{ include('coordinate/_delete_form.html.twig') }}
{% endblock %}
